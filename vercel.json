{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "redirects": [
        {
            "destination": "/initialPage.html", 
            "permanent": false 
        }
    ]
}