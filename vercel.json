{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "redirects": [
        { "source": "/public", 
        "destination": "/initialPage.html", 
        "permanent": false }
    ]
}